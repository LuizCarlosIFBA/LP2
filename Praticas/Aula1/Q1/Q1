Classe ContaBancaria
    inicio 
        atributo cpf, nomeCorrentista, saldo
    
        
        //operacoes conta
        operacao depositar(dinheiro)
        inicio
            saldo = saldo + dinheiro
        fim
    
        operacao sacar(dinheiro)
        inicio
            se(saldo>=0) entao
                inicio
                    saldo = saldo - dinheiro
                fim
        fim
    
        operacao consultar()
        inicio
            escreva nomeCorrentista
            escreva cpf
            escreva saldo 
        fim
    fim
            
             
